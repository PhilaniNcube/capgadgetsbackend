{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    },
    "orderTotal": {
      "type": "integer"
    },
    "streetAddress": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "postalCode": {
      "type": "string"
    },
    "cellNumber": {
      "type": "string"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "emailAddress": {
      "type": "email"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "paid",
        "shipped",
        "delivered"
      ]
    },
    "orderItems": {
      "type": "json"
    },
    "paymentMethod": {
      "type": "enumeration",
      "enum": [
        "Intellimali",
        "EFT"
      ]
    }
  }
}